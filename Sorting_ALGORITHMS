# BUBBLE SORT

def bubble(arr):
    for _ in range(len(arr)):
        for i in range(1,len(arr)):
            if arr[i]<arr[i-1]:
                arr[i],arr[i-1] = arr[i-1],arr[i]
                
                
---------------------------------------------------------------------------------------------------------------
# SELECTION SORT

def select_sort(arr):
    for i in range(len(arr)):
        current_min=i
        for j in range(i+1,len(arr)):
            if arr[j]<arr[current_min]:
                current_min=j
        arr[i],arr[current_min]=arr[current_min],arr[i]


---------------------------------------------------------------------------------------------------------------
# INSERTION SORT

def insert_sort(arr):
    for i in range(1,len(arr)):
        m=i
        while arr[m]<arr[m-1] and m>0:
            arr[m],arr[m-1]=arr[m-1],arr[m]
            m=m-1
            
            
---------------------------------------------------------------------------------------------------------------



